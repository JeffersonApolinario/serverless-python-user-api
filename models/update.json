{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Update One User",
  "type": "object",
  "properties": {
		"cpf": {
			"type": "string",
			"pattern": "\\d{11}"
    },
    "phoneNumber": {
      "type": "string",
      "pattern": "\\d{10, 11}"
    },
    "forceOfPassword": {
      "type": "string",
      "minLength": 3
    },
    "firstName": {
      "type": "string",
      "minLength": 3
		},
		"lastName": {
      "type": "string",
      "minLength": 3
		}
  },
  "additionalProperties": false,
  "minProperties" : 1
}