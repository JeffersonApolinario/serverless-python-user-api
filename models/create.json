{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Create User",
  "type": "object",
  "properties": {
		"email": {
			"type": "string",
			"format": "email"
		},
		"firstName": {
			"type": "string",
			"minLength": 3
		},
		"lastName": {
			"type": "string",
			"minLength": 3
		}
	},
	"additionalProperties": false,
	"required": ["email","firstName", "lastName"]
}